<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="800.0" minWidth="1000.0" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="AddProductViewController">
   <Label alignment="CENTER" contentDisplay="CENTER" layoutX="296.0" layoutY="5.0" text="Add Product"
          AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
      <font>
         <Font name="Quicksand Regular" size="30.0"/>
      </font>
   </Label>
   <Label alignment="CENTER" layoutX="13.0" layoutY="101.0" text="Name*">
      <font>
         <Font name="Quicksand Regular" size="30.0"/>
      </font>
   </Label>
   <Label alignment="CENTER" layoutX="16.0" layoutY="58.0" text="ID">
      <font>
         <Font name="Quicksand Regular" size="30.0"/>
      </font>
   </Label>
   <Label alignment="CENTER" layoutX="12.0" layoutY="152.0" text="Price Sold*">
      <font>
         <Font name="Quicksand Regular" size="30.0"/>
      </font>
   </Label>
   <Label alignment="CENTER" layoutX="-1.0" layoutY="206.0" prefHeight="40.0" prefWidth="200.0" text="Price Bought*">
      <font>
         <Font name="Quicksand Regular" size="30.0"/>
      </font>
   </Label>
   <Label alignment="CENTER" layoutX="12.0" layoutY="260.0" text="Category*">
      <font>
         <Font name="Quicksand Regular" size="30.0"/>
      </font>
   </Label>
   <Label alignment="CENTER" layoutX="16.0" layoutY="318.0" text="Stock*">
      <font>
         <Font name="Quicksand Regular" size="30.0"/>
      </font>
   </Label>
   <TextField fx:id="nameTextField" layoutX="199.0" layoutY="108.0" prefHeight="25.0" prefWidth="350.0"/>
   <TextField fx:id="price_SoldTextField" layoutX="199.0" layoutY="159.0" prefHeight="25.0" prefWidth="110.0"/>
   <TextField fx:id="price_BoughtTextField" layoutX="199.0" layoutY="213.0" prefHeight="11.0" prefWidth="85.0"/>
   <TextField fx:id="categoryTextField" layoutX="199.0" layoutY="272.0" prefHeight="25.0" prefWidth="350.0"/>
   <TextField fx:id="stockTextField" layoutX="199.0" layoutY="325.0" prefHeight="11.0" prefWidth="85.0"/>
   <TextField fx:id="idTextField" layoutX="199.0" layoutY="65.0" prefHeight="25.0" prefWidth="110.0"
              promptText="(optional)"/>
   <Button contentDisplay="CENTER" layoutY="441.0" mnemonicParsing="false" onAction="#createProduct" prefHeight="100.0"
           prefWidth="1000.0" text="Create" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
      <font>
         <Font name="Quicksand Regular" size="30.0"/>
      </font>
   </Button>
   <Label alignment="CENTER" layoutX="17.0" layoutY="359.0" text="Selves*">
      <font>
         <Font name="Quicksand Regular" size="30.0"/>
      </font>
   </Label>
   <TextField fx:id="selvesTextField" layoutX="199.0" layoutY="366.0" prefHeight="11.0" prefWidth="85.0"/>
   <Button fx:id="backButton" layoutX="16.0" layoutY="12.0" mnemonicParsing="false" onAction="#viewMainScene"
           prefHeight="25.0" prefWidth="85.0" text="Go Back"/>
</AnchorPane>
